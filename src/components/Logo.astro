---
export interface Props {
  class?: string;
}

const { class: className = '' } = Astro.props;
---

<span class:list={['flex items-center', className]}>
  <!-- Logo with golden color matching favicon (#EFBF04) -->
  <img
    src="/images/upstream/upstream-golden-logo.svg"
    alt="Upstream Labs"
    class="h-10 w-auto"
  />
</span>
